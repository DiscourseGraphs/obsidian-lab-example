views:
  - type: table
    name: All
    filters:
      and:
        - file.folder == "Projects"
    order:
      - file.name
      - status
      - targetQuestionOrHyp
  - type: table
    name: My Projects
    filters:
      and:
        - file.folder == "Projects"
        - or:
            - lead == this.file.name
            - contributors.contains(this.file.name)
    order:
      - file.name
      - status
      - lead
      - contributors
      - targetQuestionOrHyp
