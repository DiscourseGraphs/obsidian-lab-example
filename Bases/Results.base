views:
  - type: table
    name: All
    filters:
      and:
        - file.hasProperty("nodeTypeId")
        - nodeTypeId == "node_i7PzyKw2NfkA8asBfxeey"
    order:
      - file.name
      - rel_5nnUOb6wBzlAwAF2Wk-ts
      - relation_Fvz-iAdQfeqSsmpKkZBN0
  - type: table
    name: Results from this Experiment
    filters:
      and:
        - nodeTypeId == "node_i7PzyKw2NfkA8asBfxeey"
        - file.hasLink(this.file.name)
    sort:
      - property: file.name
        direction: ASC
  - type: table
    name: Results from a Project
    filters:
      or:
        - nodeTypeId == "node_LwROzkVH_Zck7ZxDgN91E"
        - nodeTypeId == "node_i7PzyKw2NfkA8asBfxeey"
